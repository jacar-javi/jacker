{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://localhost"
    },
    {
      "pattern": "^http://.*\\.example\\.com"
    },
    {
      "pattern": "^https://.*\\.example\\.com"
    },
    {
      "pattern": "^http://.*\\$\\{PUBLIC_FQDN\\}"
    },
    {
      "pattern": "^https://.*\\$\\{PUBLIC_FQDN\\}"
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206]
}
