[
  {
    "targets": [
      "https://traefik.vps1.jacarsystems.net",
      "https://grafana.vps1.jacarsystems.net",
      "https://prometheus.vps1.jacarsystems.net",
      "https://blackbox.vps1.jacarsystems.net",
      "https://alertmanager.vps1.jacarsystems.net",
      "https://portainer.vps1.jacarsystems.net",
      "https://homepage.vps1.jacarsystems.net"
    ],
    "labels": {
      "job": "blackbox-https",
      "module": "https_2xx_no_redirect",
      "environment": "production",
      "probe_type": "https"
    }
  },
  {
    "targets": [
      "traefik.vps1.jacarsystems.net:443",
      "grafana.vps1.jacarsystems.net:443",
      "prometheus.vps1.jacarsystems.net:443",
      "blackbox.vps1.jacarsystems.net:443",
      "alertmanager.vps1.jacarsystems.net:443"
    ],
    "labels": {
      "job": "blackbox-ssl",
      "module": "ssl_expiry",
      "environment": "production",
      "probe_type": "ssl_certificate"
    }
  }
]
